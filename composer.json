{
  "name": "tourze/tls-crypto-factory",
  "description": "TLS加密组件的统一工厂类，提供创建各种加密算法实例的便捷方法",
  "type": "library",
  "license": "MIT",
  "require": {
    "ext-gmp": "*",
    "ext-hash": "*",
    "ext-openssl": "*",
    "paragonie/sodium_compat": "^2.0",
    "phpseclib/phpseclib": "^3.0.43",
    "tourze/tls-crypto-asymmetric": "0.0.*",
    "tourze/tls-crypto-curves": "0.0.*",
    "tourze/tls-crypto-hash": "0.0.*",
    "tourze/tls-crypto-keyexchange": "0.0.*",
    "tourze/tls-crypto-random": "0.0.*",
    "tourze/tls-crypto-symmetric": "0.0.*",
    "tourze/tls-key-format": "0.0.*"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^11.5",
    "tourze/phpunit-base": "1.*",
    "tourze/phpunit-symfony-kernel-test": "1.0.*"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\TLSCryptoFactory\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\TLSCryptoFactory\\Tests\\": "tests/"
    }
  }
}